#!/bin/bash

# Just like `cat` but takes a taskname as arguments. The task must have been
# completed successfully before you can `cat` it.
#
# Examples
#
#     $ taskcat EBLDates
#     $ taskcat Events --iln 10
#

set -e -u

args=("$@")

OUTPUT=$(taskoutput "$@"|tail -1)
if [[ "$OUTPUT" == *mrc ]]; then
    yaz-marcdump $OUTPUT
else
    cat $OUTPUT
fi
